{
  "name": "lucide-mui",
  "version": "0.0.1",
  "packageManager": "yarn@3.4.1",
  "sideEffects": false,
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@mui/material": "^5.14.2",
    "@types/node": "^20.4.5",
    "cross-env": "^7.0.3",
    "lucide": "^0.263.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "generate": "ts-node --esm scripts/generateIcons.ts",
    "build": "yarn generate && rimraf dist && yarn build:esm && yarn build:cjs && yarn build:tsc && ts-node --esm scripts/copyFiles.ts",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts,.tsx' --out-dir 'dist/esm' --copy-files --no-copy-ignored",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts,.tsx' --out-dir 'dist/cjs'",
    "build:tsc": "tsc --project tsconfig.prod.json"
  },
  "peerDependencies": {
    "@mui/material": "^5.14.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
